/* pages/order/order.wxss */
page {
  background-color: #E8E7E9;
} 
.container{
  width: 100%;
  height: 420rpx;
  margin: 0;
  padding: 0;
  position: relative;
}
.image1{
  position: absolute;
  width: 100%;
  height: 420rpx;
  border-radius: 10rpx;
}
.image2{
  position: absolute;
  width: 150rpx;
  height: 130rpx;
  border-radius: 10rpx;
}
.up{
  text-align: center;
  z-index: 1;
  width: 100%;
  height: 100rpx;
}
.up2{
  z-index: 4;
  width: 100%;
}
.line {
  background-color:#ccc;
  border: 1px solid #ccc;
  opacity: 0.2;
  z-index: 1;
}
